{
  "name": "votetracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:assets": "babel-node tools/build-assets",
    "clean": "rimraf public",
    "dev": "webpack-dev-server --hot --inline --progress --colors --content-base src/client",
    "start": "parallelshell 'npm run start-api-dev' 'npm run dev'",
    "lint": "eslint --ext .js,.jsx .",
    "migrate": "careen",
    "migrate:up": "careen -A -e",
    "migrate:down": "careen -R -e",
    "migrate:create": "careen -C",
    "prebuild": "npm run clean && npm run build:assets",
    "prestart": "npm run clean",
    "shrinkwrap": "npm shrinkwrap && babel-node tools/fix-shrinkwrap",
    "start-api-dev": "supervisor -w src/server src/server",
    "start-api": "node src/server"
  },
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.5.2",
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "careen": "^0.1.1",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "d3-dsv": "^0.3.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "dsv-loader": "^1.0.3",
    "express": "^4.13.4",
    "express-routebuilder": "^2.1.0",
    "file-loader": "^0.8.5",
    "form-serialize": "^0.7.0",
    "fs-extra": "^0.26.5",
    "fuzzy": "^0.1.1",
    "history": "^2.0.0-rc2",
    "lodash.debounce": "^4.0.3",
    "lodash.memoize": "^4.0.1",
    "moment": "^2.11.2",
    "morgan": "^1.7.0",
    "nib": "^1.1.0",
    "node-uuid": "^1.4.7",
    "parallelshell": "^2.0.0",
    "pg-promise": "^3.2.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^2.0.0-rc5",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "superagent": "^1.8.0-beta.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-register": "^6.4.3",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^4.1.0",
    "react-hot-loader": "^1.3.0",
    "supervisor": "^0.9.1",
    "webpack-dev-server": "^1.14.1"
  }
}
